			"capIsNewExceptions": [
				"CreateDataPropertyOrThrow",
				"GroupBy",
				"OrdinaryObjectCreate",
			],
		}],
	},

	"overrides": [
		{
			"files": "aos/*.js",
			"rules": {
				"max-statements": "off",
			},
